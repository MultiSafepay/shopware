<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="multi_safepay_payment.components.document.invoice"
                 class="MltisafeMultiSafepayPayment\Components\Documents\Invoice"
                 public="true">
        </service>
        <service id="multi_safepay_payment.components.quotenumber"
                 class="MltisafeMultiSafepayPayment\Components\Quotenumber"
                 public="true">
            <argument type="service" id="shopware.number_range_incrementer"/>
            <argument type="service" id="service_container"/>
        </service>
    </services>
</container>