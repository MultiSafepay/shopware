<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="multi_safepay_payment.builder.order_request_builder"
                 class="MltisafeMultiSafepayPayment\Components\Builder\OrderRequestBuilder"
                 public="true">
            <argument type="service" id="multi_safepay_payment.builder.order_request_builder_pool"/>
            <argument type="service" id="multi_safepay_payment.components.quotenumber"/>
        </service>
        <service id="multi_safepay_payment.builder.order_request_builder_pool"
                 class="MltisafeMultiSafepayPayment\Components\Builder\OrderRequestBuilderPool"
                 public="true">
            <argument type="service" id="multi_safepay_payment.builder.order_request_builder.customer_builder"/>
            <argument type="service" id="multi_safepay_payment.builder.order_request_builder.payment_options_builder"/>
            <argument type="service" id="multi_safepay_payment.builder.order_request_builder.description_builder"/>
            <argument type="service" id="multi_safepay_payment.builder.order_request_builder.seconds_active_builder"/>
            <argument type="service" id="multi_safepay_payment.builder.order_request_builder.plugin_data_builder"/>
            <argument type="service" id="multi_safepay_payment.builder.order_request_builder.delivery_builder"/>
            <argument type="service" id="multi_safepay_payment.builder.order_request_builder.shopping_cart_builder"/>
        </service>
        <service id="multi_safepay_payment.builder.order_request_builder.customer_builder"
                 class="MltisafeMultiSafepayPayment\Components\Builder\OrderRequestBuilder\CustomerBuilder"
                 public="true">
        </service>
        <service id="multi_safepay_payment.builder.order_request_builder.payment_options_builder"
                 class="MltisafeMultiSafepayPayment\Components\Builder\OrderRequestBuilder\PaymentOptionsBuilder"
                 public="true">
        </service>
        <service id="multi_safepay_payment.builder.order_request_builder.description_builder"
                 class="MltisafeMultiSafepayPayment\Components\Builder\OrderRequestBuilder\DescriptionBuilder"
                 public="true">
        </service>
        <service id="multi_safepay_payment.builder.order_request_builder.seconds_active_builder"
                 class="MltisafeMultiSafepayPayment\Components\Builder\OrderRequestBuilder\SecondsActiveBuilder"
                 public="true">
            <argument type="service" id="shopware.plugin.cached_config_reader"/>
        </service>
        <service id="multi_safepay_payment.builder.order_request_builder.plugin_data_builder"
                 class="MltisafeMultiSafepayPayment\Components\Builder\OrderRequestBuilder\PluginDataBuilder"
                 public="true">
            <argument type="service" id="config"/>
        </service>
        <service id="multi_safepay_payment.builder.order_request_builder.delivery_builder"
                 class="MltisafeMultiSafepayPayment\Components\Builder\OrderRequestBuilder\DeliveryBuilder"
                 public="true">
        </service>
        <service id="multi_safepay_payment.builder.order_request_builder.shopping_cart_builder"
                 class="MltisafeMultiSafepayPayment\Components\Builder\OrderRequestBuilder\ShoppingCartBuilder"
                 public="true">
        </service>
    </services>
</container>
